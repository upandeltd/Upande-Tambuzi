  "css": ".label-container {\n    width: 140mm;\n    height: auto;\n    line-height: 1;\n    border: 4px solid black;\n    font-family: Arial, sans-serif;\n    text-transform: uppercase;\n    font-weight: bold;\n}\n\n.label-section {\n    display: flex;\n    flex-direction: column; /* let sections stack */\n    border-top: 2px solid black;\n    padding: 5px 0;\n    font-size: 14px;\n}\n\n.label-section .label {\n    font-size: 26px;\n    font-weight: bold;\n}\n\n.label-section span {\n    font-weight: bold;\n}\n\n.product-row {\n    display: flex;\n    justify-content: flex-start;\n    align-items: flex-start;\n\n}\n\n.label {\n    margin: 0;\n    border: 0;\n}\n\n.product-list {\n    position: absolute;\n    left: 0;\n    margin-left: 10px;\n}\n\n.variety-item {\n    margin-bottom: 4px;\n}\n\n.customer-label {\n    font-size: 45px;\n    font-weight: 900;\n    line-height: 1;\n}\n\n.customer-p-o {\n    font-size: 30px;\n}\n\n.label-header {\n    text-align: center;\n    font-size: 18px;\n    font-weight: bold;\n}\n\n.label-value {\n    padding-right: 5px;\n    font-size: 20px;\n}\n",
  "custom_format": 1,
  "default_print_language": "en",
  "disabled": 0,
  "doc_type": "Box Label",
  "docstatus": 0,
  "doctype": "Print Format",
  "font": null,
  "font_size": 14,
  "format_data": null,
  "html": "<div class=\"label-container\">\n    <div class=\"label-header\">\n        <div class='customer-label'>\n            {{ doc.customer }}\n        </div>\n        \n        <div class='customer-p-o'>\n            {{doc.customer_purchase_order}}\n        </div>\n        \n    </div>\n\n    <div class=\"label-section\">\n        <div class=\"product-row\" id=\"productRow\" style=\"display: flex; flex-direction: row\">\n            <div class='label'>\n                <span>Product</span>\n            </div>\n            \n            <div class=\"product-list\">\n                {% set seen_varieties = [] %}\n                {% for item in doc.box_item %}\n                    {% if item.variety not in seen_varieties %}\n                        {{ item.variety }}<br>\n                        {% set _ = seen_varieties.append(item.variety) %}\n                    {% endif %}\n                {% endfor %}\n            </div>\n        </div>\n    </div>\n    \n    <div class=\"label-section\">\n        <div class=\"length-row\" style=\"position: relative\">\n            <div class=\"label\">\n                <span>Length</span>\n            </div>\n            \n            <div class='label-value' style=\"position: absolute; right: 0; top: 0\">\n                {{ doc.length }}\n            </div>\n        </div>\n    </div>\n\n    <div class=\"label-section\">\n        <div class=\"\">\n            <div class=\"label\">\n                <span>Pack Rate</span> \n            </div>\n            \n            <div class='label-value'>\n                {{ doc.pack_rate }}\n            </div>\n        </div>\n        \n    </div>\n\n    <div class=\"label-section\">\n        <div class=\"label\">\n            <span>Date</span>\n        </div>\n        \n        <div class='label-value'>\n            {{ frappe.utils.formatdate(doc.date, \"dd/mm/yyyy\") }}\n        </div>\n        \n        \n    </div>\n\n    <div class=\"label-section\">\n        <div class=\"label\">\n            <span>Box</span>\n        </div>\n        <div class='label-value'>\n            {{ doc.box_number }}\n        </div>\n        \n    </div>\n</div>\n\n<script>\n  window.onload = function () {\n    const productRow = document.getElementById('productRow');\n    const items = document.querySelectorAll('.product-list-item');\n    const lineHeight = 22; // same as in CSS, or use getBoundingClientRect\n\n    if (productRow && items.length > 0) {\n      const totalHeight = items.length * lineHeight;\n      productRow.style.height = totalHeight + \"px\";\n    }\n  };\n</script>",
